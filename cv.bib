	doi = {10.1299/jsmeicone.2023.30.1036},
	note = {ISSN: 2424--2934
Issue: 0
Volume: 2023.30},
	file = {Full Text PDF:/Users/huff/Zotero/storage/23HANGGW/Okyay et al. - 2022 - HI-STORM Overpack and MPC-32 Thermal-Hydraulic Mod.pdf:application/pdf},
