	keywords = {Hydrogen, Microreactors, Nuclear hybrid systems, Thermochemical integration},
	pages = {112968},
	file = {ScienceDirect Snapshot:/Users/huff/Zotero/storage/P3HS5EYA/S0029549324000700.html:text/html},
}

