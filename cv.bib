@phdthesis{chee_fluoride-salt-cooled_2022,
	address = {Urbana, IL},
	type = {Dissertation},
	title = {Fluoride-{Salt}-{Cooled} {High} {Temperature} {Reactor} {Design} {Optimization} with {Evolutionary} {Algorithms}},
